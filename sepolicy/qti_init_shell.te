typeattribute qti_init_shell data_between_core_and_vendor_violators;
allow qti_init_shell sysfs:file write;
allow qti_init_shell vendor_radio_data_file:file { getattr open read write };
allow qti_init_shell vendor_radio_data_file:dir { getattr open search setattr read write };
allow qti_init_shell vfat:file { getattr open read setattr };
allow qti_init_shell vfat:dir { read search };
allow qti_init_shell qti_init_shell:capability { dac_override dac_read_search };
